# Increase volume
"pactl set-sink-mute 0 false ; pactl set-sink-volume 0 +5%"
   XF86AudioRaiseVolume

# Decrease volume
"pactl set-sink-mute 0 false ; pactl set-sink-volume 0 -5%"
   XF86AudioLowerVolume

# Toggle volume
"pactl set-sink-mute 0 toggle"
   XF86AudioMute

# Increase backlight
"xbacklight -inc 10"
   XF86MonBrightnessUp

# Decrease backlight
"xbacklight -dec 10"
   XF86MonBrightnessDown
